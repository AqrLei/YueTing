<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SemiRoundBar">
        <attr name="backgroundColor" format="color"/>
        <attr name="progressColor" format="color"/>
        <attr name="startDegree" format="integer"/>
        <attr name="sweepDegree" format="integer"/>
        <attr name="radius" format="dimension"/>
        <attr name="roundWidth" format="dimension"/>
        <attr name="max" format="float"/>
        <attr name="progress" format="float"/>
        <attr name="capStyle" format="integer"/>
        <attr name="openAnimation" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="LearnView">
        <attr name="learnType" format="integer"/>
        <attr name="learnTimes" format="integer"/>
        <attr name="learnWidth" format="integer"/>
        <attr name="learnHeight" format="integer"/>
        <attr name="learnScale" format="float"/>
        <attr name="learnMinScale" format="float"/>
        <attr name="learnMaxScale" format="float"/>
        <attr name="learnRadius" format="float"/>
        <attr name="learnStartX" format="float"/>
        <attr name="learnStartY" format="float"/>
        <attr name="learnLength" format="float"/>


    </declare-styleable>
</resources>